{
	"extends": "../tsconfig.base",
	"compilerOptions": {
		"types": [
			"foo"
		],
		"strictNullChecks": true,
		"noImplicitAny": true,
		"paths": {
			"$lib": [
				"*",
				"./lib"
			],
			"$src": [
				"./src"
			]
		}
	},
	"include": [
		"../src/**/*",
		"../lib/**/*"
	],
	"exclude": [
		"../../**/foo/*"
	],
	"watchOptions": {
		"watchFile": "useFsEvents",
		"watchDirectory": "useFsEvents",
		"fallbackPolling": "dynamicPriority"
	}
}